@using Inmobiliaria.Models
@model RestoreAction

<form asp-action="RestorePost" method="POST">
    <select class="form-select" aria-label="Default select example" name="RestoreId">
        @foreach (var item in ((BackupViewModel)ViewData["BackupViewModel"]).Items) {
            <option value="@item.Id">@item</option>
        }
    </select>
    <button type="submit" class='btn btn-primary'>Realizar restore</button>
</form>